{
	"include iostream": {
		"prefix": "iostream",
		"body": [
			"#include <iostream>"
		]
	},
	"include cstdio": {
		"prefix": "cstdio",
		"body": [
			"#include <cstdio>"
		]
	},
	"include cstdlib": {
		"prefix": "cstdlib",
		"body": [
			"#include <cstdlib>"
		]
	},
	"using namespace std": {
		"prefix": "using",
		"body": [
			"using namespace std;"
		]
	},
	"system pause": {
		"prefix": "pause",
		"body": "system(\"pause\");"
	},
	"Mql file header": {
		"prefix": "mqlfileheader",
		"body": [
			"//+------------------------------------------------------------------+",
			"//|                                                     ${1:fileName}.mq5 |",
			"//|                                           Copyright 2019, Ncfxy. |",
			"//|                                             https://www.mql5.com |",
			"//+------------------------------------------------------------------+",
			"$2"
		]
	},
	"Mql file default properties": {
		"prefix": "mqlfileproperty",
		"body": [
			"#property copyright \"Copyright 2018, Ncfxy.\"",
			"#property link \"https://www.mql5.com\"",
			"#property version \"1.00\""
		]
	},
	"Mql function comment": {
		"prefix": "mqlfunctioncomment",
		"body": [
			"//+------------------------------------------------------------------+",
			"//| ${1:function comment}                                                 |",
			"//+------------------------------------------------------------------+"
		]
	},
	"ncfxynew": {
		"prefix": "ncfxynew",
		"body": [
			"#include \"../RootInclude.mqh\"",
			"",
			"// $1"
		]
	}
}